{
  "name": "num-portal-webapp",
  "version": "1.22.0-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:hmr": "ng serve --hmr",
    "build": "node --max_old_space_size=6144 ./node_modules/.bin/ng build",
    "test": "npm run clean:test-results && jest",
    "test-ci": "node --expose-gc ./node_modules/.bin/jest --logHeapUsage --ci -w=1",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "pretty-quick": "pretty-quick --pattern \"src/**/*.{scss,html}\" --ignore-path=.prettierignore",
    "e2e": "ng e2e",
    "playground": "angular-playground",
    "clean:test-results": "rimraf --glob reports/**",
    "docs:build": "npm run docs:clean && npm run docs:build:en && npm run docs:build:de",
    "docs:build:en": "cd ./docs && sphinx-build -b html -D language=\"en\" . _build/html/en && cd ..",
    "docs:build:de": "cd ./docs && sphinx-build -b html -D language=\"de\" . _build/html/de && cd ..",
    "docs:clean": "rimraf ./docs/build/html/*",
    "docs:extract-messages": "cd ./docs && sphinx-build -b gettext . _gettext && cd ..",
    "docs:update-l10n": "npm run docs:extract-messages && npm run docs:update-po",
    "docs:update-po": "cd ./docs && sphinx-intl update -p _gettext && cd .."
  },
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.9",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^16.2.12",
    "@angular/material": "^16.2.9",
    "@angular/material-moment-adapter": "^16.2.9",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@babel/traverse": "^7.23.7",
    "@fortawesome/angular-fontawesome": "^0.13.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-regular-svg-icons": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@juggle/resize-observer": "^3.4.0",
    "@ng-idle/core": "^14.0.0",
    "@ng-idle/keepalive": "^14.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@swimlane/ngx-charts": "^20.5.0",
    "angular-oauth2-oidc": "^17.0.2",
    "echarts": "^5.5.1",
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "monaco-editor": "^0.51.0",
    "nanoid": "^4.0.2",
    "ngx-echarts": "^8.0.1",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.1",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.14",
    "@angular-eslint/builder": "16.3.0",
    "@angular-eslint/eslint-plugin": "16.3.0",
    "@angular-eslint/eslint-plugin-template": "16.3.0",
    "@angular-eslint/schematics": "16.3.0",
    "@angular-eslint/template-parser": "16.3.0",
    "@angular/cli": "^16.2.14",
    "@angular/compiler-cli": "^16.2.12",
    "@types/jasmine": "^5.1.4",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^16.18.72",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^8.9.0",
    "angular-playground": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^4.3.8",
    "jasmine-core": "^5.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-preset-angular": "^13.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^15.2.10",
    "lodash": "^4.17.21",
    "prettier": "3.2.4",
    "pretty-quick": "^4.0.0",
    "protractor": "~7.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern 'src/**/*.{scss,html,ts}' && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "jestSonar": {
    "reportPath": "reports/jest-sonar-reporter",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
