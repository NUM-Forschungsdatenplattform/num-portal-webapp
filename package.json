{
  "name": "num-portal-webapp",
  "version": "1.6.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:hmr": "ng serve --hmr",
    "build": "node --max_old_space_size=6144 ./node_modules/.bin/ng build",
    "test": "npm run clean:test-results && jest",
    "test-ci": "node --expose-gc ./node_modules/.bin/jest --ci -w=1",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "pretty-quick": "pretty-quick --pattern \"src/**/*.{scss,html}\"",
    "e2e": "ng e2e",
    "playground": "angular-playground",
    "clean:test-results": "rimraf reports/**"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.2.14",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@juggle/resize-observer": "^3.3.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@swimlane/ngx-charts": "^17.0.1",
    "angular-oauth2-oidc": "^10.0.3",
    "echarts": "^5.1.2",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.26.1",
    "nanoid": "^3.1.25",
    "ng-lint-staged": "^7.0.2",
    "ngx-echarts": "^7.0.2",
    "rxjs": "^6.6.6",
    "tslib": "^2.1.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.14",
    "@angular/cli": "^11.2.14",
    "@angular/compiler-cli": "~11.2.14",
    "@types/jest": "^26.0.24",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^12.20.21",
    "@types/uuid": "^8.3.1",
    "angular-playground": "^7.1.1",
    "codelyzer": "^6.0.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "jest": "^26.6.3",
    "jest-junit": "^12.2.0",
    "jest-preset-angular": "^8.4.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern 'src/**/*.{scss,html,ts}' && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "ng-lint-staged lint --fix --"
    ]
  },
  "jestSonar": {
    "reportPath": "reports/jest-sonar-reporter",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
