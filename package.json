{
  "name": "num-portal-webapp",
  "version": "1.17.0",
  "scripts": {
    "postinstall": "ngcc",
    "ng": "ng",
    "start": "ng serve",
    "start:hmr": "ng serve --hmr",
    "build": "node --max_old_space_size=6144 ./node_modules/.bin/ng build",
    "test": "npm run clean:test-results && jest",
    "test-ci": "node --expose-gc ./node_modules/.bin/jest --logHeapUsage --ci -w=1",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "pretty-quick": "pretty-quick --pattern \"src/**/*.{scss,html}\"",
    "e2e": "ng e2e",
    "playground": "angular-playground",
    "clean:test-results": "rimraf reports/**",
    "docs:build": "npm run docs:clean && npm run docs:build:en && npm run docs:build:de",
    "docs:build:en": "cd ./docs && sphinx-build -b html -D language=\"en\" . _build/html/en && cd ..",
    "docs:build:de": "cd ./docs && sphinx-build -b html -D language=\"de\" . _build/html/de && cd ..",
    "docs:clean": "rimraf ./docs/build/html/*",
    "docs:extract-messages": "cd ./docs && sphinx-build -b gettext . _gettext && cd ..",
    "docs:update-l10n": "npm run docs:extract-messages && npm run docs:update-po",
    "docs:update-po": "cd ./docs && sphinx-intl update -p _gettext && cd .."
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^14.2.13",
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/flex-layout": "^14.0.0-beta.41",
    "@angular/forms": "^14.3.0",
    "@angular/material": "^14.2.7",
    "@angular/material-moment-adapter": "^14.2.7",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@babel/traverse": "^7.23.2",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@juggle/resize-observer": "^3.3.1",
    "@ng-idle/core": "^11.1.0",
    "@ng-idle/keepalive": "^11.0.3",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@swimlane/ngx-charts": "^19.2.0",
    "angular-oauth2-oidc": "^13.0.1",
    "angular-playground": "^8.2.0",
    "crypto-browserify": "^3.12.0",
    "echarts": "^5.2.1",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "monaco-editor": "^0.29.1",
    "nanoid": "^3.1.30",
    "ng-lint-staged": "^12.0.3",
    "ngx-echarts": "^8.0.1",
    "rxjs": "^6.6.7",
    "stream-browserify": "^3.0.0",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "^14.2.13",
    "@angular/compiler-cli": "^14.3.0",
    "@types/jasmine": "^4.3.0",
    "@types/jest": "^28.1.8",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.18.69",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "jasmine-core": "^3.10.1",
    "jasmine-spec-reporter": "^7.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-junit": "^13.0.0",
    "jest-preset-angular": "^12.2.6",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^12.1.2",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "protractor": "~7.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.4.0",
    "typescript": "^4.8.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern 'src/**/*.{scss,html,ts}' && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "ng-lint-staged lint --fix --"
    ]
  },
  "jestSonar": {
    "reportPath": "reports/jest-sonar-reporter",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
