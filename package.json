{
  "name": "num-portal-webapp",
  "version": "1.7.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:hmr": "ng serve --hmr",
    "build": "node --max_old_space_size=6144 ./node_modules/.bin/ng build",
    "test": "npm run clean:test-results && jest",
    "test-ci": "node --expose-gc ./node_modules/.bin/jest --ci -w=1",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "pretty-quick": "pretty-quick --pattern \"src/**/*.{scss,html}\"",
    "e2e": "ng e2e",
    "playground": "angular-playground",
    "clean:test-results": "rimraf reports/**",
    "docs:build": "npm run docs:clean && npm run docs:build:en && npm run docs:build:de",
    "docs:build:en": "cd ./docs && sphinx-build -b html -D language=\"en\" . _build/html/en && cd ..",
    "docs:build:de": "cd ./docs && sphinx-build -b html -D language=\"de\" . _build/html/de && cd ..",
    "docs:clean": "rimraf ./docs/build/html/*",
    "docs:extract-messages": "cd ./docs && sphinx-build -b gettext . _gettext && cd ..",
    "docs:update-l10n": "npm run docs:extract-messages && npm run docs:update-po",
    "docs:update-po": "cd ./docs && sphinx-intl update -p _gettext && cd .."
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^12.2.14",
    "@angular/animations": "^12.2.14",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.14",
    "@angular/compiler": "~12.2.14",
    "@angular/core": "~12.2.14",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~12.2.14",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.2.14",
    "@angular/platform-browser-dynamic": "~12.2.14",
    "@angular/router": "~12.2.14",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@juggle/resize-observer": "^3.3.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@swimlane/ngx-charts": "^18.0.1",
    "@types/jest": "^27.0.3",
    "angular-oauth2-oidc": "^10.0.3",
    "echarts": "^5.2.1",
    "jest": "^27.4.4",
    "jest-preset-angular": "^11.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.29.1",
    "nanoid": "^3.1.30",
    "ng-lint-staged": "^7.0.3",
    "ngx-echarts": "^7.0.2",
    "rxjs": "^6.6.6",
    "ts-jest": "^27.1.1",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular/cli": "^12.2.14",
    "@angular/compiler-cli": "~12.2.14",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^12.20.21",
    "@types/uuid": "^8.3.1",
    "angular-playground": "^8.2.0",
    "codelyzer": "^6.0.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.3.0",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern 'src/**/*.{scss,html,ts}' && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "ng-lint-staged lint --fix --"
    ]
  },
  "jestSonar": {
    "reportPath": "reports/jest-sonar-reporter",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
