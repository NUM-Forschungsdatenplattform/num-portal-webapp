{
  "name": "num-portal-webapp",
  "version": "1.15.0",
  "scripts": {
    "postinstall": "ngcc",
    "ng": "ng",
    "start": "ng serve",
    "start:hmr": "ng serve --hmr",
    "build": "node --max_old_space_size=6144 ./node_modules/.bin/ng build",
    "test": "npm run clean:test-results && jest",
    "test-ci": "node --expose-gc ./node_modules/.bin/jest --logHeapUsage --ci -w=1",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "pretty-quick": "pretty-quick --pattern \"src/**/*.{scss,html}\"",
    "e2e": "ng e2e",
    "playground": "angular-playground",
    "clean:test-results": "rimraf reports/**",
    "docs:build": "npm run docs:clean && npm run docs:build:en && npm run docs:build:de",
    "docs:build:en": "cd ./docs && sphinx-build -b html -D language=\"en\" . _build/html/en && cd ..",
    "docs:build:de": "cd ./docs && sphinx-build -b html -D language=\"de\" . _build/html/de && cd ..",
    "docs:clean": "rimraf ./docs/build/html/*",
    "docs:extract-messages": "cd ./docs && sphinx-build -b gettext . _gettext && cd ..",
    "docs:update-l10n": "npm run docs:extract-messages && npm run docs:update-po",
    "docs:update-po": "cd ./docs && sphinx-intl update -p _gettext && cd ..",
    "packagr": "ng-packagr -p ng-package.json target='es2015' module='es2015'"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^16.1.8",
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.11",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/cli": "~16.1.0",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "16.1.9",
    "@angular/material": "^17.0.4",
    "@angular/material-moment-adapter": "^17.0.4",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@vitagroup/design-system-angular": "^0.1.3",
    "@angular/router": "^16.2.12",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@juggle/resize-observer": "^3.3.1",
    "@ng-idle/core": "^13.0.0",
    "@ng-idle/keepalive": "^13.0.0",
    "@swimlane/ngx-charts": "^20.4.1",
    "@types/jasmine": "^4.3.0",
    "@types/jest": "^27.0.3",
    "angular-oauth2-oidc": "^13.0.1",
    "angular-playground": "^8.2.0",
    "echarts": "^5.2.1",
    "jest": "^29.7.0",
    "jest-junit": "^13.0.0",
    "jest-preset-angular": "^11.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "monaco-editor": "^0.29.1",
    "nanoid": "^3.1.30",
    "ng-lint-staged": "^12.0.3",
    "ngx-echarts": "^8.0.1",
    "rxjs": "^6.6.7",
    "ts-jest": "^29.1.1",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "^16.0.2",
    "@angular-eslint/eslint-plugin-template": "^16.0.2",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "^16.0.2",
    "@angular/cli": "~16.1.0",
    "@angular/compiler-cli": "^16.2.12",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.12",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "jasmine-core": "^3.10.1",
    "jasmine-spec-reporter": "^7.0.0",
    "lint-staged": "^12.1.2",
    "lodash": "^4.17.21",
    "ng-packagr": "^16.2.3",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "protractor": "~7.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "5.1.6"
  },
  "overrides": {
    "node-forge": ">=1.3.0",
    "terser": ">=5.14.2",
    "node-fetch": ">=2.6.7",
    "d3-color": ">=3.1.0",
    "webpack": "^5.76.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern 'src/**/*.{scss,html,ts}' && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "ng-lint-staged lint --fix --"
    ]
  },
  "jestSonar": {
    "reportPath": "reports/jest-sonar-reporter",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}